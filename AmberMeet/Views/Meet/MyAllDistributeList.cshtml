@{
    ViewBag.Title = "我的发起";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="input-group" style="margin-bottom: 3px;">
    <div class="input-control has-icon-left has-icon-right">
        <input id="searchText" class="form-control" type="search" placeholder="关键字" style="width: 80%">
        <label for="searchText" class="input-control-icon-left search-icon">
            <i class="icon icon-search"></i>
        </label>
        <input type="text" id="activateDate" name="activateDate" class="form-control" placeholder="开会日期" style="width: 10%"/>
        <select class="form-control" id="meetStates" style="padding-left: 0; padding-right: 0; width: 10%;">
            <option value="">选择</option>
            @{
                foreach (var meetState in ViewBag.meetStates)
                {
                    <option value="@meetState.Key">@meetState.Value</option>
                }
            }
        </select>
    </div>
    <span class="input-group-btn">
        <button type="button" id="searchButton" class="btn btn-primary">搜索</button>
    </span>
</div>
<div class="jqGrid_wrapper">
    <table id="allDistributeList"></table>
    <div id="allDistributeListPager"></div>
</div>
<script src="~/Views/Meet/MyAllDistributeList.js"></script>